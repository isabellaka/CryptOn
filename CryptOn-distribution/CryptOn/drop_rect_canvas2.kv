FloatLayout:
    size_hint: 0.3, 0.5
    pos_hint: {"top":0.65, "right":0.35}

    canvas.before:

        Color:
            rgba: 1, 1, 1, 1
        Rectangle:
            size: self.size
            pos: self.pos
        Color:
            rgba: 0, 0.4, 0.5, 1
        Line:
            width: 2
            rectangle: self.x, self.y, self.width, self.height